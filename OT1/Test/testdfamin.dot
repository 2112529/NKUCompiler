digraph DFA {
    rankdir=LR;
    node [shape = circle];
    
    // States
    S0 [label="0"];
    S1 [label="1"];
    S2 [label="2"];
    S3 [label="3,4", shape = doublecircle];

    // Transitions
    S0 -> S1 [label="b"];
    S0 -> S2 [label="a"];
    S1 -> S1 [label="b"];
    S1 -> S2 [label="a"];
    S2 -> S3 [label="a"];
    S2 -> S3 [label="b"];
}
